            <SCRIPT language=javascript>
 function judge()
    {
            if (document.frm1.catalogid1.value=="-1")
                { alert("请选择分类");
                 document.frm1.catalogid1.focus();
                 return false;
                 }
     }
</SCRIPT>

            <SCRIPT language=javascript src="soft.files/subcatalog.js">
</SCRIPT>

            <SCRIPT language=JavaScript>
function selectCatalog()
{
        var Brandbox;
        var Modelbox;
        var strj,strd;
        var i;
        Brandbox = document.frm1.catalogid1;
        selBrand = Brandbox.options[Brandbox.selectedIndex];

        var    tNameIdx = asubCatalogNameIdx[Brandbox.selectedIndex -1];
        var    tValueIdx = asubCatalogValueIdx[Brandbox.selectedIndex -1];

        Modelbox = document.frm1.catalogid2;
        for(i = 1; i<=tNameIdx.length; i++) {
             strj = tNameIdx[i-1];
             strd = tValueIdx[i-1];
             Modelbox.options[i] = new Option(strj,strd);
        }
        Modelbox.length = i;
}
function gotoBigCatalogByExpress(obj) {
    with(obj) {
        var url = "http://www.cseek.com/catalog/entrance.do";
        //var rid = regionid.options[regionid.selectedIndex].value;
        /*if(parseInt(rid) < 1) {
            rid = 1;
        }*/

        url += "?catalogid=" + catalogid2.options[catalogid2.selectedIndex].value;
        url += "&regionid=1" ;
        window.open(url,'','');
    }

}
</SCRIPT>

