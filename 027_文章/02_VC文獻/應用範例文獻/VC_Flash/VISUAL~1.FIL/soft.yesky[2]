                    <OPTION value=-1 selected>所 选 大 类</OPTION> <OPTION 
                    value=113>整机</OPTION> <OPTION value=117>办公与外设</OPTION> 
                    <OPTION value=158>网络设备</OPTION> <OPTION 
                    value=211>配件</OPTION> <OPTION value=253>数码</OPTION> <OPTION 
                    value=290>通讯产品</OPTION> <OPTION value=549>移动存储</OPTION> 
                    <OPTION value=717>无线网</OPTION> <OPTION 
                  value=1095>耗材</OPTION></SELECT> </TD>
                <TD vAlign=right width=105><SELECT 
